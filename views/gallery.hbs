<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Gallery</title>
    <link href="https://fonts.googleapis.com/css?family=Lato|Sigmar+One" rel="stylesheet">
    <link rel="stylesheet" href="gallery-style.css">
  </head>
  <body>
    <nav>
      <h3 class="nav-title">Beast Jesus</h3>
      <div class="links">
        <a class="prof" href="/user">Your Profile</a>
        <a class="gallery" href="/pixel_page">Pixelate It</a>
      </div>
    </nav>
    <div class="page-container">
      <div class="container">
    <aside class="art-gallery">
      <div class="three-pics">
        {{#each data}}
        <img  class="gallery-art" src="{{this.url}}" alt="{{this.title}}">
        {{/each}}
      </div>
    </aside>
    <main class="pixel-display">
      <div class="pixel-display-pics">
        <!-- <img src="./images/wave_disasterpiece.png" alt="">
        <img src="./images/venus_disasterpiece.png" alt=""> -->
        {{#each item}}
          <img class="pixel-art" src="{{this.div_data}}">
          <form class="delete" action="/gallery/{{this.id}}/delete/" method="get">
            <button type="submit" class="delete-pixels">Delete</button>
          </form>
        {{/each}}
      </div>
    </main>
  </div>
  </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
  </body>
</html>
